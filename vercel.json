{
    "version": 2,
    "builds": [
        {
            "src": "client/package.json",
            "use": "@vercel/node"
        },
        {
            "src": "client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "rewrites": [
        {
            "source": "/account/:path*",
            "destination": "/"
        },
        {
            "source": "/:path*",
            "destination": "/"
        }
    ]
}